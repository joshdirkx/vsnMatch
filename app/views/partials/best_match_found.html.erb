<table>
  <tbody class="best-match-results">
    <th>
      Most Specific Result
	</th>
    <% @best_match.each do |match| %>
      <tr>
        <td><%= match.id %></td>
        <td><%= match.vsn_pattern %></td>
        <td><%= match.trim_id %></td>
        <td><%= match.year %></td>
        <td><%= match.make %></td>
        <td><%= match.model %></td>
        <td><%= match.trim_name %></td>
      </tr>
    <% end %>
  </tbody>
  <th  id='expand'>
    We found <%= @results.count %> total options, click to see them
  </th>
  <tbody style='display:none' id='options' class='expanded-query-results'>
    <tr>
      <td>Inventory ID</td>
      <td>VSN Pattern</td>
      <td>Trim ID</td>
      <td>Year</td>
      <td>Make</td>
      <td>Model</td>
      <td>Trim Name</td>
    </tr>
    <% @results.each do |i| %>
      <tr>
        <td><%= i.id %></td>
        <td><%= i.vsn_pattern %></td>
        <td><%= i.trim_id %></td>
        <td><%= i.year %></td>
        <td><%= i.make %></td>
        <td><%= i.model %></td>
        <td><%= i.trim_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>